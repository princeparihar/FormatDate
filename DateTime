package DateFormat;


import java.util.*;
import java.text.*;

public class DateFormat {

   public static void main(String args[]) {
      Date live = new Date( );
      SimpleDateFormat ft = 
      new SimpleDateFormat ("E dd.MM.yyyy 'at' ss:mm:hh a zzz");

      System.out.println("Current Date: " + ft.format(live));
   }
}
